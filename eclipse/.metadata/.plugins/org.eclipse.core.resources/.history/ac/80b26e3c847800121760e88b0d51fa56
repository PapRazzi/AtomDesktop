package me.atom.windowsj;

import java.io.*;
import java.net.*;

public class SubmitFileWithID {

	public static void submit(String id) {
		
		try {
			URLConnection connection = new URL("http://atom.alwaysdata.net/guzzoni/process.php?id=" + id).openConnection();
			connection.setRequestProperty("Accept-Charset", "UTF-8");
			connection.getInputStream();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}
	
}
